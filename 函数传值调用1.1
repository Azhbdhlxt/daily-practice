//程序健壮性 
#include<stdio.h>
#include<stdlib.h>
#include<math.h>
#include<graphics.h>
#include<time.h>
long Fact(int n); 

int main(int argc, char* argv[])
{
    int m;
    
	long ret;
	
	printf("输入m:");
	
	scanf("%d", &m);
	
	ret = Fact(m); 
	 //调用函数Fact(),并将函数返回值存入ret 
    printf("%d != %ld\n", m, ret);

  return 0;
}
long Fact(int n)
//函数定义 
	{
		int i;
		
		long result = 1;
		
		if (n < 0)
		{
			printf("输入数据错误"); 
		 } 
		 else
		 {
		 for (i = 2; i <= n; i ++)
		 
         result *= i;
         
		return result;
		
	}
	 }
