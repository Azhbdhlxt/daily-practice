#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#include <graphics.h>
#include <time.h>
int main(int argc, char* argv[])
{
    int x;
    
   for (x = 1; ; x ++ )
   {
   	if (x % 5 == 1 && x % 6 == 5 && x % 7 == 4 && x % 11  == 10)
   	
   	{
   		printf("x = % d\n", x );
   		break;
		//找到第一个满足关系式的解后立即退出循环 
	   }
   }

  return 0;
}
